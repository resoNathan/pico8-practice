pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
function _init()
	cls(15)
	x = 63
	y = 63
end

function _draw()
	if btn(❎) then cls(15) end
	
	if btn(⬅️) then x -= 1 end
	if btn(➡️) then x += 1 end
	if btn(⬆️) then y -= 1 end
	if btn(⬇️) then y += 1 end
	
	circfill(x,y,1,0)
end
